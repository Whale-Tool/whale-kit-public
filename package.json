  },
  "devDependencies": {
    "@skpm/builder": "^0.5.11"
  },
  "author": "jingwhale <jingwhale@yeah.net>",
  "dependencies": {
    "sketch-module-web-view": "^2.1.7",
    "tinycolor2": "^1.4.1"
  }
}
