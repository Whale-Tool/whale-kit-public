{
  "name": "whale-kit",
  "version": "0.1.0",
  "engines": {
    "sketch": ">=3.0"
  },
  "skpm": {
    "name": "whale-kit",
    "manifest": "src/manifest.json",
    "main": "whale-kit.sketchplugin",
    "assets": [
      "assets/**/*"
    ]
